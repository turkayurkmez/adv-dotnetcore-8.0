@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<div>
    <h3>Controller içerisindeki injection sonuçları</h3>
    <hr />
    <p>Singleton: @ViewBag.SingletonGuid</p>
    <p>Scoped: @ViewBag.ScopedGuid</p>
    <p>Transient: @ViewBag.TransientGuid</p>

</div>

<div>
    <h3>Service Injection sonuçları</h3>
    <hr />
    <p>Singleton: @ViewBag.ServiceSingletonGuid</p>
    <p>Scoped: @ViewBag.ServiceScopedGuid</p>
    <p>Transient: @ViewBag.ServiceTransientGuid</p>
</div>

